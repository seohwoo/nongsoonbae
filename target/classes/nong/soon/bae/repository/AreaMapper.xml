<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="nong.soon.bae.repository.AreaMapper">
	<select id="catelist" resultType="nong.soon.bae.bean.AreaDTO">
		select * from area where area2=0
	</select>
	
	<select id="catelistarea" resultType="nong.soon.bae.bean.AreaDTO">
		select * from area where area1=#{area1} and area2>0
	</select>
	
</mapper>
